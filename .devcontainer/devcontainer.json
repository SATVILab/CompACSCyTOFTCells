{
	"image": "ghcr.io/rocker-org/devcontainer/geospatial:4",
	"features": {
		"ghcr.io/MiguelRodo/DevContainerFeatures/apptainer:1": {},
		"ghcr.io/MiguelRodo/DevContainerFeatures/git-xet:1": {},
		"ghcr.io/MiguelRodo/DevContainerFeatures/repos:1": {},
		"ghcr.io/MiguelRodo/DevContainerFeatures/config-r:1": {}
	},
	
	"customizations": {
		"vscode": {
			"extensions": [
				"quarto.quarto",
				"GitHub.copilot",
				"GitHub.copilot-chat",
				"REditorSupport.r",
				"bierner.markdown-preview-github-styles",
				"eamodio.gitlens",
				"mathematic.vscode-pdf"
			]
		},
		// Configure properties specific to Codespaces.
		"codespaces": {
			"repositories": {
				"SATVILab/DataTidyACSClinical": {
					"permissions": {
						"contents": "write",
						"issues": "write",
						"metadata": "read",
						"pullRequests": "write"
					}
				}
			}
		}
	}
}