{
	"image": "ghcr.io/rocker-org/devcontainer/geospatial:4.2",
	"features": {
		"ghcr.io/MiguelRodo/DevContainerFeatures/apptainer:1": {},
		"ghcr.io/MiguelRodo/DevContainerFeatures/git-xet:1": {},
		"ghcr.io/MiguelRodo/DevContainerFeatures/repos:1": {},
		"ghcr.io/MiguelRodo/DevContainerFeatures/config-r:1": {}
	},
	
	"customizations": {
		"vscode": {
			"extensions": [
				"quarto.quarto",
				"GitHub.copilot",
				"GitHub.copilot-chat",
				"REditorSupport.r",
				"bierner.markdown-preview-github-styles",
				"eamodio.gitlens",
				"mathematic.vscode-pdf"
			]
		},
		// Configure properties specific to Codespaces.
		"codespaces": {
			"repositories": {
				"SATVILab/DataTidyACSClinical": {
					"permissions": "write-all"
				},
				"SATVILab/DataTidyACSCyTOFPreprocess": {
					"permissions": "write-all"
				},
				"SATVILab/DataTidyACSCyTOFFAUST": {
					"permissions": "write-all"
				},
				"SATVILab/PipelineDataACSCytokines": {
					"permissions": "write-all"
				},
				"SATVILab/DataTidyACSCyTOFCytokinesTCells": {
					"permissions": "write-all"
				},
				"SATVILab/PipelineAnalysisACS": {
					"permissions": "write-all"
				},
				"SATVILab/AnalysisACSCyTOFTCells": {
					"permissions": "write-all"
				},
				"SATVILab/ReportACSCyTOFTCells": {
					"permissions": "write-all"
				}
			}
		}
	}
}